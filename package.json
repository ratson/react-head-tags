{
  "name": "react-head-tags",
  "version": "0.0.4",
  "description": "ReactDOMServer.renderToNodeStream() ready component to manage document.head",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "clean": "del lib dist coverage",
    "prepublish": "run-s clean build",
    "lint": "eslint --ignore-path .gitignore --ext js,md .",
    "size": "size-limit",
    "pretest": "npm run build",
    "test": "run-p test:* lint",
    "test:jest": "jest",
    "test:size": "run-s build size",
    "precommit": "lint-staged"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "fbjs": "^0.8.16",
    "nanoevents": "^1.0.2",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.35",
    "@babel/core": "^7.0.0-beta.35",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.35",
    "@babel/preset-env": "^7.0.0-beta.35",
    "@babel/preset-react": "^7.0.0-beta.35",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "cross-env": "^5.1.3",
    "del-cli": "^1.1.0",
    "delay": "^2.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-react-16-adapter-setup": "^0.1.0",
    "eslint": "^4.13.1",
    "eslint-config-concise": "^0.16.1",
    "eslint-config-concise-esnext": "^0.16.1",
    "eslint-config-concise-jest": "^0.16.1",
    "eslint-config-concise-react": "^0.16.3",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "jest-enzyme": "^4.0.1",
    "lint-staged": "^6.0.0",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.7.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "size-limit": "^0.13.2",
    "stream-to-promise": "^2.2.0"
  },
  "repository": "ratson/react-head-tags",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "keywords": [
    "base",
    "document",
    "head",
    "isomorphic",
    "link",
    "meta",
    "nfl",
    "noscript",
    "portals",
    "react",
    "react-component",
    "react-head-tags",
    "react-helmet",
    "script",
    "ssr",
    "style",
    "tags",
    "title"
  ],
  "browserslist": [
    "last 2 versions",
    "IE >= 9"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "raf/polyfill",
      "enzyme-react-16-adapter-setup"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "3 KB"
    }
  ]
}
